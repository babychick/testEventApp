{
  "expo": {
    "name": "Sukien",
    "description": "This project is really great.",
    "slug": "TodoApp",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "splash": {
      "image": "./images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "images/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ndphi.ToDoApp",
      "adaptiveIcon": {
        "backgroundImage": "./images/icon.png",
        "foregroundImage": "./images/icon.png"
      }
    }
  }
}